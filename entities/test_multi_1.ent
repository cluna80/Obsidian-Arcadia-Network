
ENTITY MultiBot1
STATE Active
ENERGY 80
REPUTATION 2

BEHAVIOR
  IF ENERGY < 40 THEN STATE Recovery
  IF REPUTATION > 3 THEN STATE Elite
END

EXECUTE
  IF STATE == Active THEN DataCollector
  IF STATE == Recovery THEN Monitor
  IF STATE == Elite THEN DeepAnalyzer
END

INTENT "Multi-entity behavior test 1"
MODE Testing
